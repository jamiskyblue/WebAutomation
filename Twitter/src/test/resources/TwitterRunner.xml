<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE suite SYSTEM "http://testng.org/testng-1.0.dtd">

<suite name="Selenium Test Suite" parallel="false">
    <parameter name="platform" value="mac"/>
    <parameter name="cloud" value="false"/>
    <parameter name="envName" value="browserstack"/>
    <parameter name="browser" value="chrome"/>
    <parameter name="browserVersion" value="77.0"/>
    <parameter name="url" value="https://www.twitter.com"/>


    <test name="Automation tests">
        <classes>
            <class name="homepagetests.OpenBrowserTest"/>
        </classes>
    </test>
</suite>